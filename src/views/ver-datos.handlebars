<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}}</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <div class="container">
        <h1>{{title}}</h1>
        <a href="/"><button>HOME</button></a>
        <table>
            <thead>
                <tr>
                    <th>CLIENTE</th>
                    <th>ZONA</th>
                    <th>GPO</th>
                    <th>NOMBRE</th>
                    <th>DIRECCION</th>
                    <th>DIRECCION_REMITO</th>
                    <th>LOCALIDAD</th>
                    <th>CP</th>
                    <th>TEL</th>
                    <th>EMAIL</th>
                    <th>COORDENADAS</th>
                </tr>
            </thead>
            <tbody>
                {{#each data}}
                <tr>
                    <td>{{this.CLIENTE}}</td>
                    <td>{{this.ZONA}}</td>
                    <td>{{this.GPO}}</td>
                    <td>{{this.NOMBRE}}</td>
                    <td>{{this.DIRECCION}}</td>
                    <td>{{this.DIRECCION_REMITO}}</td>
                    <td>{{this.LOCALIDAD}}</td>
                    <td>{{this.CP}}</td>
                    <td>{{this.TEL}}</td>
                    <td>{{this.EMAIL}}</td>
                    <td>{{this.COORDENADAS}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</body>
</html>
